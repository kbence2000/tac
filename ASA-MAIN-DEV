entities/CodeGeneration.json
{
  "name": "CodeGeneration",
  "type": "object",
  "properties": {
    "title": {
      "type": "string",
      "description": "Generation title"
    },
    "prompt": {
      "type": "string",
      "description": "User prompt for code generation"
    },
    "language": {
      "type": "string",
      "enum": ["python", "javascript", "typescript", "rust", "go", "java", "cpp", "csharp"],
      "description": "Programming language"
    },
    "framework": {
      "type": "string",
      "description": "Framework or library"
    },
    "generated_code": {
      "type": "string",
      "description": "Generated code output"
    },
    "status": {
      "type": "string",
      "enum": ["pending", "generating", "completed", "error", "reviewing"],
      "default": "pending",
      "description": "Generation status"
    },
    "quality_score": {
      "type": "number",
      "minimum": 0,
      "maximum": 100,
      "description": "Code quality score"
    },
    "suggestions": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "AI suggestions for improvement"
    },
    "bugs_found": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Detected bugs"
    },
    "test_coverage": {
      "type": "number",
      "default": 0,
      "description": "Test coverage percentage"
    },
    "complexity_score": {
      "type": "number",
      "description": "Code complexity metric"
    },
    "collaborators": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "Active collaborators"
    }
  },
  "required": ["title", "prompt", "language"]
}
entities/CICDPipeline.json
{
  "name": "CICDPipeline",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Pipeline name"
    },
    "project_name": {
      "type": "string",
      "description": "Associated project"
    },
    "status": {
      "type": "string",
      "enum": ["idle", "running", "success", "failed", "cancelled"],
      "default": "idle",
      "description": "Pipeline status"
    },
    "stage": {
      "type": "string",
      "enum": ["build", "test", "deploy", "completed"],
      "description": "Current stage"
    },
    "build_number": {
      "type": "number",
      "description": "Build number"
    },
    "duration": {
      "type": "number",
      "description": "Duration in seconds"
    },
    "coverage": {
      "type": "number",
      "description": "Test coverage"
    },
    "tests_passed": {
      "type": "number",
      "default": 0,
      "description": "Passed tests"
    },
    "tests_failed": {
      "type": "number",
      "default": 0,
      "description": "Failed tests"
    },
    "deploy_url": {
      "type": "string",
      "description": "Deployment URL"
    },
    "logs": {
      "type": "array",
      "items": {
        "type": "object"
      },
      "description": "Pipeline logs"
    }
  },
  "required": ["name", "project_name"]
}
