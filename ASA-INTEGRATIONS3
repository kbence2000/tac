// JavaScript Example: Reading Entities
// Filterable fields: name, trigger_type, trigger_config, action_type, target_modules, enabled, last_triggered, execution_count
async function fetchAutomationRuleEntities() {
    const response = await fetch(`https://app.base44.com/api/apps/691f52903c699af7047b818f/entities/AutomationRule`, {
        headers: {
            'api_key': '7eef83b9870f4bff9d102dcd41c979e1', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        }
    });
    const data = await response.json();
    console.log(data);
}

// JavaScript Example: Updating an Entity
// Filterable fields: name, trigger_type, trigger_config, action_type, target_modules, enabled, last_triggered, execution_count
async function updateAutomationRuleEntity(entityId, updateData) {
    const response = await fetch(`https://app.base44.com/api/apps/691f52903c699af7047b818f/entities/AutomationRule/${entityId}`, {
        method: 'PUT',
        headers: {
            'api_key': '7eef83b9870f4bff9d102dcd41c979e1', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(updateData)
    });
    const data = await response.json();
    console.log(data);
}